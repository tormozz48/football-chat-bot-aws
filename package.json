{
  "name": "football-chat-bot-aws",
  "version": "1.0.0",
  "description": "Football Chat Bot. Integrated into Amazon Web Services",
  "author": {
    "name": "Andrey Kuznetsov",
    "email": "tormozz48@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:tormozz48/football-chat-bot-aws.git"
  },
  "license": "MIT",
  "keywords": [
    "NodeJS",
    "AWS Lambda",
    "TypeScript",
    "Serverless",
    "DynamoDB",
    "Telegram",
    "Bot",
    "VK"
  ],
  "scripts": {
    "lint": "eslint --max-warnings 0 'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "dev": "serverless offline",
    "deploy": "serverless deploy",
    "pretest": "serverless dynamodb migrate",
    "test": "jest --silent",
    "test:debug": "jest --runInBand",
    "test:ci": "jest --silent --coverage --runInBand",
    "coverage": "jest --coverage",
    "db-start": "serverless dynamodb start --migrate",
    "db-gui": "DYNAMO_ENDPOINT=http://localhost:8000 dynamodb-admin"
  },
  "dependencies": {
    "@aws-sdk/lib-dynamodb": "^3.76.0",
    "aws-sdk": "^2.1120.0",
    "handlebars": "^4.7.7",
    "lambda-log": "^3.1.0",
    "luxon": "^2.3.2",
    "serverless-dynamodb-client": "0.0.2",
    "serverless-http": "^2.7.0",
    "telegraf": "^4.7.0",
    "telegraf-aws": "^1.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.94",
    "@types/faker": "5.5.3",
    "@types/jest": "^27.4.1",
    "@types/lambda-log": "^2.2.1",
    "@types/lambda-wrapper": "^0.3.1",
    "@types/luxon": "^2.3.1",
    "@types/serverless-jest-plugin": "^0.3.0",
    "@typescript-eslint/parser": "^5.19.0",
    "dynamodb-admin": "^4.1.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "5.5.3",
    "jest": "^27.5.1",
    "jest-environment-serverless": "^0.3.0",
    "prettier": "^2.6.2",
    "serverless": "^3.16.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-jest-plugin": "^0.4.0",
    "serverless-offline": "^8.7.0",
    "serverless-plugin-typescript": "^2.1.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
